{
  "name": "github-explorer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "polished": "^3.6.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.1",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/tests/"
    ],
    "testMatch": [
      "**/tests/**/*.spec.tsx"
    ],
    "coverageDirectory": "tests/coverage",
    "collectCoverageFrom": [
      "!src/*.tsx",
      "!src/styles/*.ts",
      "!src/services/*.ts",
      "!src/routes/*.tsx"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/factory-girl": "^5.0.2",
    "@types/faker": "^4.1.12",
    "@types/jest": "^24.0.0",
    "axios-mock-adapter": "^1.18.1",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
    "ts-jest": "^26.1.1"
  }
}
